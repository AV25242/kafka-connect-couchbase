{
  "name": "test-couchbase-source",
  "config": {
    "name": "test-couchbase-source",
    "connector.class": "com.couchbase.connect.kafka.CouchbaseSourceConnector",
    "tasks.max": "2",
    "topic.name": "test-default",
    "couchbase.seedNodes": "127.0.0.1",
    "couchbase.bootstrapTimeout": "2000",
    "couchbase.bucket": "default",
    "couchbase.username": "Administrator",
    "couchbase.password": "password",
    "couchbase.sourceHandler": "com.couchbase.connect.kafka.handler.source.DefaultSchemaSourceHandler",
    "couchbase.eventFilter": "com.couchbase.connect.kafka.filter.AllPassFilter",
    "couchbase.streamFrom": "SAVED_OFFSET_OR_BEGINNING",
    "couchbase.compression": "ENABLED",
    "couchbase.flowControlBuffer": "128m",
    "couchbase.persistencePollingInterval": "100ms"
  }
}
